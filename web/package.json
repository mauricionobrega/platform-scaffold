    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "sw-toolbox": "3.4.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-core": "6.21.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.21.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-to-json": "0.6.0",
    "ignore-styles": "5.0.1",
    "imagemin-power-cli": "0.4.0",
    "ip": "1.1.4",
    "jest-cli": "18.0.0",
    "jsdom": "9.9.1",
    "json-loader": "0.5.4",
    "lighthouse": "1.6.0",
    "lodash.assign": "4.2.0",
    "lodash.throttle": "4.1.1",
    "minimist": "1.2.0",
    "mobify-code-style": "2.7.3",
    "nightwatch": "0.9.8",
    "nightwatch-commands": "2.0.0",
    "node-sass": "4.1.1",
    "postcss-loader": "1.2.1",
    "progressive-web-sdk": "0.13.2",
    "prompt": "1.0.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.1",
    "react-styleguidist": "4.1.0",
    "redux-form": "6.0.5",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "4.1.0",
    "spline-scss": "2.2.0",
    "style-loader": "0.13.1",
    "susy": "2.2.12",
    "svg-sprite": "1.3.6",
    "svgo": "0.7.1",
    "text-loader": "0.0.1",
    "validator": "6.2.0",
    "webpack": "2.2.0",
    "webpack-bundle-analyzer": "2.3.0",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.13.0",
    "html-webpack-plugin": "2.28.0"
  },
  "config": {
    "sdk_generator_version": 2,
    "min_lighthouse_score": 40
  },
  "scripts": {
    "add:component": "sdk-generate component",
    "add:form": "sdk-generate form",
    "add:page": "sdk-generate page",
    "build-sprites": "npm run clean:svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --dest=./app/static/svg/sprite-dist ./app/static/svg/sprite-source/*.svg",
    "clean:svgs": "svgo --enable='removeUselessStrokeAndFill' -f ./app/static/svg/sprite-source",
    "clean:images": "imagemin-power -v --recursive ./app/static/img/**/* --out-dir=.",
    "dev": "sdk-get-routes && sdk-create-hash-manifest && node ./dev-server/index.js",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:sass": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v",
    "push": "cross-env NODE_ENV=production npm run prod:build && sdk-upload",
    "styleguide": "styleguidist --config webpack/styleguide.config.js server",
    "save-credentials": "sdk-save-credentials",
    "smoke-test": "./tests/system/smoke-test.sh",
    "test": "cross-env NODE_ENV=test jest",
    "test:all": "npm run lint && npm test -- --coverage",
    "test:e2e": "node ./node_modules/nightwatch/bin/runner.js -c ./tests/system/nightwatch-config.js --suiteRetries 1",
    "test:pwa-prod": "./scripts/lighthouse-prod.sh",
    "test:pwa-ci": "./scripts/lighthouse-ci.sh",
    "test:pwa-local": "./scripts/lighthouse-local.sh",
    "test:watch": "npm test -- --watch",
    "prod:build": "./scripts/build.sh",
    "update-loader-routes": "sdk-get-routes"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!app/static/js/*.js",
      "!app/vendor/*.js",
      "!app/containers/reducers.js",
      "!app/**/container.{js,jsx}",
      "!{dev-server,lighthouse,coverage,build,tests,webpack}/**/*.js",
      "!styleguide/**/*.{js,jsx}",
      "!app/loader-routes.js",
      "!web/worker/main.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 11,
        "functions": 31,
        "lines": 50,
        "statements": 47
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/tests/emptyStringMock.js"
    },
    "setupFiles": [
      "<rootDir>/tests/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/webpack/",
      "/tmp/",
      "/node_modules/",
      "/app/static/",
      "/app/vendor/",
      "/dev-server/",
      "/lighthouse/",
      "/coverage/"
    ],
    "testRegex": "\\.?(test|spec)\\.jsx?$"
  }
}
