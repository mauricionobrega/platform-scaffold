  "private": true,
  "version": "0.13.1",
  "dependencies": {
    "babel-runtime": "6.11.6",
    "es6-promise": "4.0.5",
    "immutable": "3.8.1",
    "lodash.frompairs": "4.0.1",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "redux": "3.6.0",
